#include <iostream>

using namespace std;

class Agenda {
    private:
    int telefone;
    string nome;
    
    public:
}

    //Constructor
    Agenda() {
    telefone = 12345678;
    nome = "Maria";
    
    //Getters & Setters
    int getTelefone(){
    return this->telefone;
    }

    void setTelefone (int t) {
    this->telefone = t;
    }

    string getNome(){
    return this->nome;
    }

    void setNome (string n) {
    this->nome = n;
    
    //MÃ©todo
    void imprimir();
};

int main()
{
    Agenda c1;
    
    c1.setTelefone(12345678);
    c1.setNome("Maria");
    
    cout << c1.telefone << endl;
    cout << c1.nome << endl;
    

    return 0;
}

void Agenda::imprimir(){
    cout << telefone << endl;
    cout << nome << endl;
};
